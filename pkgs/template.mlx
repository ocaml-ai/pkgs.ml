open Jsx

let title = text "Pkgs.ml"

let render_package (package: Package_search_index.document) =
  <h2>(text package.id)</h2>

let homepage ~(packages: Package_search_index.document list) =
  let package_els = List.map render_package packages in
  <html lang="en">
    <head>
      <meta charSet="utf-8"/>
      <title>(title)</title>
    </head>
    <body>
      <h1>(title)</h1>

      <div>
      (list package_els)
      </div>
    </body>
  </html>
